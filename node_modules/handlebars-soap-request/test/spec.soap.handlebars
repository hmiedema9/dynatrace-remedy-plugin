<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope" xmlns:soap1="http://mydomain.com/ws/soapheaders">
	<soap:Header>
		<soap1:locale>en_AU</soap1:locale>
	</soap:Header>
	<soap:Body>
		<name>{{{cdata name}}}</name>
		{{#with postalAddress}}
			{{> address type="postal"}}
		{{/with}}
		{{#with billingAddress}}
			{{> address type="billing"}}
		{{/with}}
	</soap:Body>
</soap:Envelope>
